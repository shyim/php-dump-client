{
  "name": "shyim/php-dump-client",
  "license": "mit",
  "autoload": {
    "psr-4": {
      "PhpDumpClient\\": "src/"
    },
    "files": ["functions.php"]
  },
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "doctrine/sql-formatter": "^1.1",
    "symfony/var-dumper": "5.4.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "doctrine/dbal": "*"
  },
  "config": {
    "sort-packages": true,
    "platform-check": false,
    "platform": {
        "php": "7.2.5"
    }
  }
}
